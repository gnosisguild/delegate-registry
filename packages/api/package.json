    "prisma": "5.10.2",
    "vercel": "34.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "prettier": "3.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
